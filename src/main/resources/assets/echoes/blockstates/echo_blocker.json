{
  "multipart": [
    {
      "when": {"north": false},
      "apply": {
        "model": "echoes:block/echo_blocker_face"
      }
    },
    {
      "when": {"east": false},
      "apply": {
        "model": "echoes:block/echo_blocker_face",
        "y": 90
      }
    },
    {
      "when": {"south": false},
      "apply": {
        "model": "echoes:block/echo_blocker_face",
        "y": 180
      }
    },
    {
      "when": {"west": false},
      "apply": {
        "model": "echoes:block/echo_blocker_face",
        "y": 270
      }
    },
    {
      "when": {"up": false},
      "apply": {
        "model": "echoes:block/echo_blocker_face",
        "x": 270
      }
    },
    {
      "when": {"down": false},
      "apply": {
        "model": "echoes:block/echo_blocker_face",
        "x": 90
      }
    },

    {
      "when": {"north": true},
      "apply": {
        "model": "echoes:block/echo_blocker_face_dusted"
      }
    },
    {
      "when": {"east": true},
      "apply": {
        "model": "echoes:block/echo_blocker_face_dusted",
        "y": 90
      }
    },
    {
      "when": {"south": true},
      "apply": {
        "model": "echoes:block/echo_blocker_face_dusted",
        "y": 180
      }
    },
    {
      "when": {"west": true},
      "apply": {
        "model": "echoes:block/echo_blocker_face_dusted",
        "y": 270
      }
    },
    {
      "when": {"up": true},
      "apply": {
        "model": "echoes:block/echo_blocker_face_dusted",
        "x": 270
      }
    },
    {
      "when": {"down": true},
      "apply": {
        "model": "echoes:block/echo_blocker_face_dusted",
        "x": 90
      }
    }
  ]
}